{
  "name": "common-backend-starter",
  "version": "1.0.0",
  "description": "Common Backend Project can be used as a library",
  "main": "index.js",
  "scripts": {
    "build": "npm run tsc",
    "semantic-release": "semantic-release",
    "start": "npm run build && node dist/index.js",
    "tsc": "tsc -p tsconfig-build.json",
    "lint": "eslint src test",
    "lint:fix": "eslint src/*.ts src/**/*.ts --fix",
    "test": "NODE_ENV=test mocha -r ts-node/register test/unit/index.ts ./tsconfig.json --timeout 10000 --exit",
    "test:integration": "NODE_ENV=test mocha -r ts-node/register test/integration/index.ts -p ./tsconfig.json --timeout 55000 --exit",
    "test:coverage": "NODE_ENV=test nyc --reporter=lcov mocha -r ts-node/register test/unit/index.ts -p ./tsconfig.json --timeout 55000 --exit",
    "test:coverage:codeclimate": "NODE_ENV=test nyc --reporter=lcov mocha -r ts-node/register test/unit/index.ts -p ./tsconfig.json --timeout 55000 --exit && $GITHUB_WORKSPACE/tmp/cc-test-reporter format-coverage -t lcov -o $GITHUB_WORKSPACE/tmp/codeclimate.core.json coverage/lcov.info",
    "checkLicenses": "license-checker --production --failOn 'AGPL 3.0;LGPL 2.1'",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rrahulraja/common-backend-starter.git"
  },
  "author": "Rrahul Raja <rahulraja.ngp@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rrahulraja/common-backend-starter/issues"
  },
  "homepage": "https://github.com/rrahulraja/common-backend-starter#readme",
  "dependencies": {
    "@sentry/node": "^6.11.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "express-prometheus-middleware": "^1.2.0",
    "helmet": "^4.6.0",
    "js-yaml": "^4.1.0",
    "lodash.get": "^4.4.2",
    "lodash.pick": "^4.4.0",
    "nocache": "^3.0.1",
    "node-fetch": "^2.6.1",
    "pino": "^6.13.0",
    "prom-client": "^13.2.0",
    "serialize-error": "^8.1.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-prometheus-middleware": "^1.2.1",
    "@types/helmet": "^4.0.0",
    "@types/lodash.get": "^4.4.6",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.6.1",
    "@types/node-fetch": "^2.5.12",
    "@types/pino": "^6.3.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "license-checker": "^25.0.1",
    "mocha": "^9.0.3",
    "nock": "^13.1.1",
    "node-mocks-http": "^1.10.1",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5",
    "semantic-release": "^17.4.2"
  }
}
